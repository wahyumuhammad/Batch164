@model MiniProject.VM.MCompanyVM

@{
    ViewBag.Title = "Delete Company";
}

@using (Html.BeginForm("Delete", "MCompany", FormMethod.Post, new { @id = "form-company" }))
{
    @Html.HiddenFor(m=>m.id)
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true , "", new { @class="text-danger"})

    @Html.HiddenFor(m=>m.name)
    @Html.HiddenFor(m => m.code)
    <div class="form-group" style="text-align:center">
        <button type="submit" class="btn btn-info">Delete</button>
        <a href="@Url.Action("Index","MCompany")"><button type="button" class="btn btn-warning">Cancel</button></a>
    </div>



}